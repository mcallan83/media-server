upstream plex {
  server 127.0.0.1:32400;
}

server {
  listen   80;
  server_name    _;
  access_log  /var/log/nginx/access.log;

  location /deluge {
    proxy_pass  http://127.0.0.1:8112/;
    proxy_set_header  X-Deluge-Base "/deluge/";
    proxy_cookie_path / /deluge;
    include /etc/nginx/proxy.conf;
  }

